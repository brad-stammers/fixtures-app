<div class="container" id="main">
  <div class="card-deck">
    {{#each @model as |comp|}}
      <div class="card" style="width: 18rem">
        <div class="card-header">
          <h5 class="card-title">{{comp.name}}</h5>
        </div>
        <div class="card-body">
          <h6 class="card-subtitle mb-2 text-muted">{{comp.sport}}</h6>
          <p class="card-text">Location:  {{comp.location}}</p>
          <p class="card-text">Start Date:  {{comp.startDate}}</p>
          <p class="card-text">End Date:  {{comp.endDate}}</p>
        </div>
      </div>
    {{/each}}
  </div>
</div>
